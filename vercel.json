{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/dist/$1",
        "headers": {
          "x-robots-tag": "noindex"
        }
      }
    ]
  }